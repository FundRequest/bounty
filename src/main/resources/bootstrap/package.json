{
  "name": "bootstrap-material-design",
  "description": "Material Design for Bootstrap 4",
  "version": "4.1.1",
  "keywords": [
    "material",
    "design",
    "bootstrap",
    "css",
    "js",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/FezVrasta/bootstrap-material-design",
  "author": "Federico Zivolo",
  "scripts": {
    "css": "npm-run-all --parallel css-compile* --sequential css-prefix* css-minify*",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"../static/assets/css/*.css\" \"!../static/assets/css/*.min.css\"",
    "css-compile": "node-sass --importer=node_modules/node-sass-tilde-importer scss/bootstrap-material-design.scss  ../static/assets/css/bmd.css",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output ../static/assets/css/bmd.min.css ../static/assets/css/bmd.css",
    "watch-css": "nodemon -e scss -x \"npm run css\"",
    "watch-dev": "nodemon -e scss -x \"npm run css-compile\""
  },
  "style": "dist/css/bootstrap-material-design.css",
  "sass": "scss/_core.scss",
  "main": "dist/js/bootstrap-material-design.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FezVrasta/bootstrap-material-design.git"
  },
  "bugs": {
    "url": "https://github.com/FezVrasta/bootstrap-material-design/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.25.0",
    "babel-minify": "^0.2.0",
    "babel-plugin-module-resolver": "3",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "bootstrap": "^4.0.0",
    "clean-css-cli": "^4.1.11",
    "cssnano-cli": "^1.0.5",
    "jquery": ">=3.0.0",
    "node-sass": "^4.5.3",
    "node-sass-tilde-importer": "^1.0.0",
    "nodemon": "^1.14.10",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1",
    "prettier": "^1.5.3",
    "rollup": "^0.53.4",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2"
  },
  "files": [
    "dist",
    "js/*.js",
    "scss/**/*.scss",
    "LICENSE"
  ]
}
